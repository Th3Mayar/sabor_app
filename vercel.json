{
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/node",
      "config": {
        "includeFiles": ["**/**"]
      }
    }
  ],
  "buildCommand": "npm run build",
  "installCommand": "npm install",
  "outputDirectory": "FRONTEND/.output/public",
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/BACKEND/dist/app.js"
    },
    {
      "src": "/(.*)",
      "dest": "/FRONTEND/$1"
    }
  ]
}
