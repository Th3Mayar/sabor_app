{
  "version": 2,
  "builds": [{ "src": "./app.ts", "use": "@vercel/node" }],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/",
      "methods": ["GET", "POST", "PUT", "PATCH", "DELETE", "OPTIONS"]
    }
  ],
  "redirects": [
    { "source": "/api/(.*)", "destination": "/api/$1", "statusCode": 200 }
  ]
}
